{
    "name": "gpupo/questions-for-developers",
    "type": "library",
    "description": "Training questions for developers",
    "keywords": [
        "symfony",
        "cli",
        "test",
        "exam"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "repositories":
    [
        {
            "type": "vcs",
            "url": "http://github.com/P3PO/certificationy-cli"
        }
    ],
    "require": {
        "php": ">=7.3",
        "gpupo/common": "dev-master",
        "gpupo/common-sdk": "dev-master",
        "certificationy/certificationy-cli": "v2.1.x-dev",
        "certificationy/php-pack": "dev-master"

    },
    "require-dev": {
      "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\QuestionsForDevelopers\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
